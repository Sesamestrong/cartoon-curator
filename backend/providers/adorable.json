{
    "series-ids": ["adorable"],
    "date-scrape": [{
        "headers": {
            "uri": "https://echo-info.herokuapp.com/",
            "method": "POST",
            "form": {
                "date": "$convert{\"${year}-${month}-${day}\",\"YYYY-MM-DD\",\"YYYYMMDD\"}"
            }
        },
        "json": {
            "%date": "src"
        }
    }, {
        "headers": {
            "uri": "https://get-adjacent-days.herokuapp.com",
            "method": "POST",
            "json": {
                "year": "${year}",
                "month": "${month}",
                "day": "${day}"
            }
        },
        "json": {
            "%previous": 0,
            "%next": 0
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "http://avatars.adorable.io/"
        },
        "frame": {
            "%name": "h1"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "https://api.adorable.io/avatars/1600/${src}",
    "id": "adorable",
    "name": "Adorable"
}
