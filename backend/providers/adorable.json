{
    "series-ids": ["adorable"],
    "date-scrape": [{
        "set": {
            "%src": "$convert{\"${year}-${month}-${day}\",\"YYYY-MM-DD\",\"YYYYMMDD\"}"
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "http://avatars.adorable.io/"
        },
        "frame": {
            "%name": "h1"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "https://api.adorable.io/avatars/1600/${src}",
    "id": "adorable",
    "name": "Adorable",
    "description": "Randomly generated adorable avatars."
}
