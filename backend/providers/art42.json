{
    "series-ids": ["cubism"],
    "date-scrape": [{
        "headers": {
            "uri": "https://art42.net/"
        },
        "frame": {
            "%src": ".img-wrapper > img @ src || [^/]+/[^/]+(?=\\.jpg$)"
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "https://art42.net/"
        },
        "frame": {
            "%name": "title || ^[^|]+"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "https://vcloud42.com/file/art42-cdn/${src}.jpg",
    "id": "art42",
    "name": "Art42"
}
