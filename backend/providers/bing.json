{
    "series-ids": [],
    "date-scrape": [{
        "headers": {
            "uri": "https://get-bing-id.herokuapp.com/",
            "method": "POST",
            "json": {
                "year": "${year}",
                "month": "${month}",
                "day": "${day}"
            }
        },
        "text": "%id"
    }, {
        "headers": {
            "uri": "https://bing.com/HPImageArchive.aspx?format=js&idx=${id}&n=1&mkt=${seriesId}"
        },
        "json": {
            "images": {
                "%url": "src"
            }
        }
    }, {
        "headers": {
            "uri": "https://get-adjacent-days.herokuapp.com",
            "method": "POST",
            "json": {
                "year": "${year}",
                "month": "${month}",
                "day": "${day}"
            }
        },
        "json": {
            "%previous": 1,
            "%next": 1
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": [{
            "headers": {
                "uri": "https://bing.com/HPImageArchive.aspx?format=js&idx=0&n=1&mkt=${seriesId}"
            }
        }],
        "json": {
            "images": {
                "%enddate": "last"
            }
        }
    },
    "get-name": [{
        "headers": {
            "uri": "http://www.lingoes.net/en/translator/langcode.htm"
        },
        "frame": {
            "%name": "td:contains('${seriesId}') ~ td"
        }
    }],
    "date-formats": ["YYYY-MM-DD", "YYYY[]MM[]DD"],
    "list-names": [{
        "headers": {
            "uri": "http://www.lingoes.net/en/translator/langcode.htm"
        },
        "frame": {
            "%seriesIds": ["td:nth-child(1)"]
        }
    }],
    "src-to-url": "https://bing.com${src}",
    "id": "bing",
    "name": "Bing Daily Images"
}
