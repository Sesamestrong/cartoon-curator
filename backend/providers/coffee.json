{
    "series-ids": ["coffee"],
    "date-scrape": [{
        "headers": {
            "uri": "https://coffee.alexflipnote.dev/random.json"
        },
        "json": {
            "%file": "src"
        }
    }, {
        "headers": {
            "uri": "https://get-adjacent-days.herokuapp.com",
            "method": "POST",
            "json": {
                "year": "${year}",
                "month": "${month}",
                "day": "${day}"
            }
        },
        "json": {
            "%previous": 0,
            "%next": 0
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "https://coffee.alexflipnote.dev/"
        },
        "frame": {
            "%name": "h1.title"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "${src}",
    "id": "coffee",
    "name": "Coffee API"
}
