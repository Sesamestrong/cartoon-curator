{
    "series-ids": ["dog"],
    "date-scrape": [{
        "headers": {
            "uri": "https://dog.ceo/api/breeds/image/random"
        },
        "json": {
            "%message": "src"
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "https://dog.ceo/dog-api"
        },
        "frame": {
            "%name": "h1"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "${src}",
    "id": "dog-api",
    "name": "Dog API"
}
