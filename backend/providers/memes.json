{
    "series-ids": ["dankmemes", "programmerhumor", "meirl", "memes", "deepfriedmemes"],
    "date-scrape": [{
        "headers": {
            "uri": "https://meme-api.herokuapp.com/gimme/${seriesId}"
        },
        "json": {
            "%url": "src"
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "https://meme-api.herokuapp.com/gimme/${seriesId}"
        },
        "json": {
            "%subreddit": "name"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "${src}",
    "id": "memes",
    "name": "Meme API"
}
