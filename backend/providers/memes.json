{
    "series-ids": ["dankmemes", "programmerhumor", "meirl", "memes", "deepfriedmemes"],
    "date-scrape": [{
        "headers": {
            "uri": "https://meme-api.herokuapp.com/gimme/${seriesId}"
        },
        "json": {
            "%url": "src"
        }
    }, {
        "headers": {
            "uri": "https://get-adjacent-days.herokuapp.com",
            "method": "POST",
            "json": {
                "year": "${year}",
                "month": "${month}",
                "day": "${day}"
            }
        },
        "json": {
            "%previous": 0,
            "%next": 0
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "https://meme-api.herokuapp.com/gimme/${seriesId}"
        },
        "json": {
            "%subreddit": "name"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "${src}",
    "id": "memes",
    "name": "Meme API"
}
