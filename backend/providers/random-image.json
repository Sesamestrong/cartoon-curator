{
    "series-ids": ["default", "grayscale", "blur", "grayscale&blur"],
    "date-scrape": [{
        "headers": {
            "uri": "https://echo-info.herokuapp.com/",
            "method": "POST",
            "json": {
                "src": "$convert{\"${year}-${month}-${day}\",\"YYYY-MM-DD\",\"YYYYMMDD\"}/1000/700?${seriesId}"
            }
        },
        "json": {
            "%src": 0
        }
    }, {
        "headers": {
            "uri": "https://get-adjacent-days.herokuapp.com",
            "method": "POST",
            "json": {
                "year": "${year}",
                "month": "${month}",
                "day": "${day}"
            }
        },
        "json": {
            "%previous": 0,
            "%next": 0
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "https://picsum.photos/"
        },
        "frame": {
            "%name": "title"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "https://picsum.photos/seed/${src}",
    "id": "random-image",
    "name": "Random Image"
}
