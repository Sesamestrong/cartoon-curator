{
    "series-ids": ["shiba"],
    "date-scrape": [{
        "headers": {
            "uri": "https://shibe.online/api/shibes?count=1&urls=true&httpsUrls=true"
        },
        "json": {
            "%0": "src"
        }
    }, {
        "headers": {
            "uri": "https://get-adjacent-days.herokuapp.com",
            "method": "POST",
            "json": {
                "year": "${year}",
                "month": "${month}",
                "day": "${day}"
            }
        },
        "json": {
            "%previous": 0,
            "%next": 0
        }
    }],
    "extremes-scrape": {
        "first": [],
        "last": []
    },
    "get-name": [{
        "headers": {
            "uri": "https://legitcode267.github.io/react-random-shiba/"
        },
        "frame": {
            "%name": "title"
        }
    }],
    "date-formats": ["YYYY-MM-DD"],
    "src-to-url": "${src}",
    "id": "shibas",
    "name": "Shibas"
}
